{
  "version": "1.0",
  "brainShareSystem": {
    "storage": {
      "memoryDirectory": "/Volumes/DATA/GitHub/rEngine/rMemory/rAgentMemories",
      "coreFiles": [
        "handoff.json",
        "extendedcontext.json",
        "tasks.json",
        "decisions.json"
      ]
    },
    "smartScribe": {
      "scriptPath": "/Volumes/DATA/GitHub/rEngine/rEngine/smart-scribe.js",
      "localLlm": {
        "provider": "ollama",
        "model": "qwen2.5-coder:7b",
        "endpoint": "http://localhost:11434"
      }
    },
    "protocols": {
      "startupScript": "/Volumes/DATA/GitHub/rEngine/rEngine/one-click-startup.js",
      "checkpointScript": "/Volumes/DATA/GitHub/rEngine/scripts/git-checkpoint.sh"
    },
    "documentManager": {
      "scriptPath": "/Volumes/DATA/GitHub/rEngine/rEngine/document-generator.js",
      "api": {
        "providers": {
          "groq": {
            "name": "Groq",
            "endpoint": "https://api.groq.com/openai/v1/chat/completions",
            "models": ["llama3-8b-8192", "mixtral-8x7b-32768", "gemma-7b-it"],
            "defaultModel": "llama3-8b-8192",
            "maxTokens": 6000
          },
          "openai": {
            "name": "OpenAI",
            "endpoint": "https://api.openai.com/v1/chat/completions",
            "models": ["gpt-3.5-turbo", "gpt-4", "gpt-4-turbo"],
            "defaultModel": "gpt-3.5-turbo",
            "maxTokens": 4096
          },
          "gemini": {
            "name": "Gemini",
            "endpoint": "https://generativelanguage.googleapis.com/v1beta/models/gemini-1.5-flash:generateContent",
            "models": ["gemini-1.5-flash", "gemini-1.5-pro"],
            "defaultModel": "gemini-1.5-flash",
            "maxTokens": 8192
          },
          "claude": {
            "name": "Claude",
            "endpoint": "https://api.anthropic.com/v1/messages",
            "models": ["claude-3-5-sonnet-20240620", "claude-3-haiku-20240307"],
            "defaultModel": "claude-3-haiku-20240307",
            "maxTokens": 8192,
            "rateLimits": {
              "requestsPerMinute": 50,
              "tokensPerMinute": 40000,
              "delayBetween": 1.2
            }
          }
        },
        "defaultProvider": "claude",
        "fallbackOrder": ["groq", "gemini", "openai"]
      }
    },
    "splitScribeConsole": {
      "scriptPath": "/Volumes/DATA/GitHub/rEngine/rEngine/split-scribe-console.js",
      "logFile": "/Volumes/DATA/GitHub/rEngine/scribe.log"
    }
  }
}
