<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.stacktrackr.mcp-servers</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/Volumes/DATA/GitHub/StackTrackr/rEngine/start-mcp-servers.sh</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>
    
    <key>StandardOutPath</key>
    <string>/Volumes/DATA/GitHub/StackTrackr/rEngine/launchd.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Volumes/DATA/GitHub/StackTrackr/rEngine/launchd.log</string>
    
    <key>WorkingDirectory</key>
    <string>/Volumes/DATA/GitHub/StackTrackr/rEngine</string>
    
    <key>StartInterval</key>
    <integer>300</integer>
    
    <key>UserName</key>
    <string>lbruton</string>
