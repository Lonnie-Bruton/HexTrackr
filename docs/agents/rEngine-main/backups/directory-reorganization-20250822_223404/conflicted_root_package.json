{
  "name": "stacktrackr-rengine-platform",
  "version": "3.04.87",
  "description": "StackTrackr precious metals inventory with rEngine Platform v2.1.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo 'No tests specified' && exit 1",
    "start": "node index.js",
  "dev": "nodemon index.js",
  "code:audit": "node tools/code-audit.js",
  "code:audit:pdf": "node tools/generate-pdf.js docs/code-review/RENGINE_CODE_REVIEW.html docs/code-review/RENGINE_CODE_REVIEW.pdf",
    "mobile-dev": "NODE_ENV=mobile npm start",
    "mobile-checkout": "node scripts/mobile-checkout.js",
    "mobile-checkin": "node scripts/mobile-checkin.js",
    "docker:start": "./docker/docker-dev.sh start",
    "docker:stop": "./docker/docker-dev.sh stop",
    "docker:restart": "./docker/docker-dev.sh restart",
    "docker:build": "./docker/docker-dev.sh build",
    "docker:logs": "./docker/docker-dev.sh logs",
    "docker:shell": "./docker/docker-dev.sh shell",
    "docker:vscode": "./docker/docker-dev.sh vscode",
    "docker:clean": "./docker/docker-dev.sh clean",
    "docs:publish": "node rEngine/publish-docs.js",
