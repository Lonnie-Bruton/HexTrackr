  "esprima": "^4.0.1",
    "archiver": "^7.0.1",
    "axios": "^1.7.2",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.3",
    "marked": "^16.2.0",
    "playwright": "^1.55.0",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "inventory",
    "precious-metals",
    "rengine",
    "ai-platform",
    "docker"
