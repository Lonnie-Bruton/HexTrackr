{
  "env": {
    "node": true,
    "es2021": true,
    "browser": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "require": "readonly",
    "process": "readonly",
    "__dirname": "readonly",
    "fetch": "readonly",
    "console": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
